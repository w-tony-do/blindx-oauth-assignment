{
  "name": "blindx",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "packageManager": "pnpm@10.0.0",
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "api:dev": "tsx watch prescription/src/main.ts",
    "api:build": "node esbuild.config.mjs",
    "api:start": "NODE_ENV=production node out/index.js",
    "api:test": "jest",
    "api:coverage": "jest --coverage",
    "api:watch-test": "jest --watch",
    "db:generate": "kysely-codegen",
    "db:migrate": "tsx src/libs/db/migrate.ts",
    "db:seed": "tsx src/libs/db/seed.ts",
    "ui:dev": "vite",
    "ui:build": "tsc && vite build",
    "ui:test": "vitest run",
    "ui:coverage": "vitest --coverage",
    "ui:watch-test": "vitest --watch",
    "lint": "eslint . --ext .ts,.tsx --report-unused-disable-directives --max-warnings 0",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@fastify/cors": "^9.0.1",
    "@fastify/redis": "^7.1.0",
    "@ts-rest/core": "^3.51.0",
    "@ts-rest/fastify": "^3.52.1",
    "dotenv": "^16.4.5",
    "fastify": "^4.28.1",
    "ioredis": "^5.8.1",
    "kysely": "^0.27.4",
    "pg": "^8.12.0",
    "pino-pretty": "^11.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tsx": "^4.20.6",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/ioredis-mock": "^8.2.6",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.14.10",
    "@types/pg": "^8.11.6",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "@typescript-eslint/parser": "^8.46.2",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-v8": "^3.2.4",
    "esbuild": "^0.25.11",
    "eslint": "^8.57.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.0",
    "ioredis-mock": "^8.13.0",
    "jest": "^29.7.0",
    "jsdom": "^24.0.0",
    "kysely-codegen": "^0.15.0",
    "ts-jest": "^29.1.2",
    "typescript": "5.9.2",
    "vite": "^7.1.11",
    "vitest": "^3.2.4"
  }
}
