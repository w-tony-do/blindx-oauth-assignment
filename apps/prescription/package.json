{
  "name": "prescription",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "tsx watch src/main.ts",
    "build": "node esbuild.config.js",
    "start": "tsx src/main.ts",
    "start:prod": "NODE_ENV=production node out/index.js",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "lint": "eslint . --ext .ts,.tsx",
    "check-types": "tsc --noEmit",
    "db:generate": "kysely-codegen",
    "db:migrate": "tsx src/libs/db/migrate.ts",
    "db:seed": "tsx src/libs/db/seed.ts"
  },
  "dependencies": {
    "@fastify/cors": "^9.0.1",
    "@fastify/redis": "^7.1.0",
    "@repo/contracts": "workspace:*",
    "@ts-rest/core": "^3.51.0",
    "@ts-rest/fastify": "^3.52.1",
    "dotenv": "^16.4.5",
    "fastify": "^4.28.1",
    "ioredis": "^5.8.1",
    "kysely": "^0.27.4",
    "pg": "^8.12.0",
    "pino-pretty": "^11.0.0",
    "tsx": "^4.20.6",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@repo/eslint-config": "workspace:*",
    "@repo/typescript-config": "workspace:*",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.14.10",
    "@types/pg": "^8.11.6",
    "esbuild": "^0.25.11",
    "eslint": "^8.57.0",
    "ioredis-mock": "^8.13.0",
    "jest": "^29.7.0",
    "kysely-codegen": "^0.15.0",
    "ts-jest": "^29.1.2",
    "typescript": "5.9.2"
  }
}
